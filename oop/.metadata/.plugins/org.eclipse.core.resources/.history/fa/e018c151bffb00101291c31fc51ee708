package practice_1;

import java.util.Scanner; ;

public class GradeSystem {
	public static void main(String args[]) {
		
		Scanner gradeInput = new Scanner(System.in);
		
		int number ;
		String grade ;
		
		System.out.print("Enter grade number: ") ;
		number = input.nextInt() ;
		
		
		
		if(number >= 95) grade = "A" ;
		if(number >= 90) grade = "A-" ;
		if(number >= 85) grade = "B+" ;
		if(number >= 80) grade = "B" ;
		if(number >= 75) grade = "B-" ;
		if(number >= 70) grade = "C+" ;
		if(number >= 65) grade = "C" ;
		if(number >= 60) grade = "C-" ;
		if(number >= 55) grade = "D+" ;
		if(number >= 50) grade = "D" ;
		if(number >= 25) grade = "FX" ;
		else grade = "F" ;
		
		System.out.print("Your grade: " + grade) ;
		
		
	}
}
